{
	"tasks": [
		{
		  // type를 shell로 설정
		  "type": "shell",
		  // custom label를 설정
		  "label": "Build minishell_test",
		  // make 명령어를 실행!
		  "command": "/usr/bin/make",
		  // 현재 workspace를 기준으로 re, debug_make 작업을 수행함
		  "args": ["-C", "${workspaceFolder}/", "re", "all"],
		  "options": {
			"cwd": "/usr/bin",
			// 환경 변수로 DEBUG: ture로 설정함
			"env": {
			  "DEBUG": "1",
			//   "SANI": "1"
			}
		  },
		  "problemMatcher": ["$gcc"],
		  "group": {
			"kind": "build",
			"isDefault": true
		  }
		}
	  ],
	  "version": "2.0.0"
}