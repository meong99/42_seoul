{
    "tasks": [
        {
            "type": "shell",
            "label": "Build minishell_test",
            "command": "/usr/bin/make",
            "args": [
                "-C",
                "${workspaceFolder}",
                "re",
            ],
            "options": {
                "cwd": "/usr/bin",
                "env": {
                    "DEBUG": "1",
                    "CPPFLAGS": "-I/opt/homebrew/opt/readline/include",
                    "LDFLAGS": "-L/opt/homebrew/opt/readline/lib"
                }
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": "build"
        }
    ],
        "version": "2.0.0"
