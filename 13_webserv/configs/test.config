server
{
	server_name default
	listen 8176 127.0.0.1			

	location /						
	{
		error_page 404 ./tests/test1/error404.html
		error_page 405 ./tests/test1/error405.html
		allow_methods GET DELETE
		root ./tests/test1/
		index index.html index2.html
		auto_index off 
		cgi_info .bla ./tests/tester_bin/cgi_tester
		cgi_info .php ./tests/tester_bin/php-cgi
	}
	
	location /directory/						
	{
		error_page 404 ./tests/test1/error404.html
		error_page 405 ./tests/test1/error405.html
		allow_methods GET DELETE
		root ./tests/test1/directory/
		index index.html index2.html
		auto_index off
		cgi_info .bla ./tests/tester_bin/cgi_tester
		cgi_info .php ./tests/tester_bin/php-cgi
	}
	
	location /directory/nop/						
	{
		error_page 404 ./tests/test1/error404.html
		error_page 405 ./tests/test1/error405.html
		allow_methods GET DELETE
		root ./tests/test1/directory/
		index index.html index2.html
		auto_index on
		cgi_info .bla ./tests/tester_bin/cgi_tester
		cgi_info .php ./tests/tester_bin/php-cgi
	}
	
	location /auth/
	{
		allow_methods GET POST DELETE
		request_max_body_size	42
		root ./tests/test1/
		cgi_info .php	./tests/tester_bin/php-cgi
		auth_key selim:1234
	}
}

server
{
	server_name second
	listen 8178 127.0.0.1
	
	location /test
	{
		allow_methods GET
		root ./tests/test1/
		index second.html
	}
}

server
{
	server_name google.com
	listen 8177 127.0.0.1

	location /
	{
		allow_methods GET
		return 301 http://google.com
	}
}